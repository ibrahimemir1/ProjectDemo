@using EntityLayer.Concrete

<div class="row">
    <div class="col-md-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Şubelerimiz</h4>
                <div class="table-responsive">
                    <table class="table">
                        <thead>
                            <tr>
                                <th> DövizKodu </th>
                                <th> Birim </th>
                                <th> DövizAlış </th>
                                <th> DövizSatış </th>
                                @*  <th>toplam çalışan sayısı</th> *@


                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td> USD/TRY</td>
                                    <td> 1</td>
                                    <td> @ViewBag.usd</td>
                                    <td> @ViewBag.usdsatıs</td>
                                   
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
