@model EntityLayer.Concrete.Branch

@{
    ViewData["Title"] = "AddEmployee";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<h1>Çalışan Oluşturma Sayfası</h1>
<br />
@using (Html.BeginRouteForm(
  routeName: "Admin",
  routeValues: new { action = "AddBranch", controller = "Branch", area = "Admin" },
  method: FormMethod.Post))
{

    <br />
    <br />
    @Html.Label("Şube İsmi")
    @Html.TextBoxFor(x => x.Name, new { @class = "form-control" })
    <br />
    <br />
    @Html.Label("Şube Adresi")
    @Html.TextBoxFor(x => x.Adress, new { @class = "form-control" })
    <br />
    <br />
    <button class="btn btn-info">Oluştur</button>
}
